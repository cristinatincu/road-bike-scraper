<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ProductComparison" table="road_bike">
        <id name="id" type="int">
            <column name="id"/>
            <generator class="increment"/>
        </id>
        <property name="roadBikeId" type="int">
            <column name="road_bike_id"/>
        </property>
        <property name="size" type="string">
            <column name="size"/>
        </property>
        <property name="price" type="float">
            <column name="price"/>
        </property>
        <property name="url" type="string">
            <column name="url"/>
        </property>
    </class>
</hibernate-mapping>
